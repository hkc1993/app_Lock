<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <com.example.huangkuncan.applicationlock.View.View.TopView
        android:id="@+id/set_success_topview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.example.huangkuncan.applicationlock.View.View.TopView>

    <TextView
        android:id="@+id/set_success_password"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="50dp"
        android:background="@drawable/shape_btn_short"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textSize="@dimen/top_view_tv_dimen"
        android:text="@string/change_password"
        />

    <TextView
        android:id="@+id/set_success_app"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:background="@drawable/shape_btn_short"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textSize="@dimen/top_view_tv_dimen"

        android:text="@string/change_app"
        />
</LinearLayout>